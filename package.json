{
  "name": "o-c-p",
  "main": "./lib/o-c-p",
  "version": "0.2.1",
  "description": "Ollama Chat Pannel, a pannel to chat with IA who turn on Ollama Server",
  "keywords": [],
  "activationCommands": {
    "atom-workspace": "o-c-p:toggle"
  },
  "repository": "https://github.com/rioxo/o-c-p.git",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "uuid": ">=11.1.0"
  },
  "package-deps": [
    "busy"
  ],
  "configSchema": {
    "ollamaServerUri": {
      "order": 0,
      "description": "The Uri of the Ollama server",
      "type": "string",
      "default": "http://localhost:11434"
    }
  },
  "consumedServices": {
    "busy": {
      "versions": {
        ">=0.0.2": "consumeBusy"
      }
    }
  }
}
